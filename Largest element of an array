#include<stdio.h>
int main()
{
	int a[50],i,n,largest;
	printf("Enter the size of array= ");
	scanf("%d",&n);
	printf("The elements are\n");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	largest=a[0];
	for(i=0;i<n;i++)
	{
		if(largest<a[i])
		{
			largest=a[i];
		}
	}
	printf("largest= %d",largest);
	return 0;
}
